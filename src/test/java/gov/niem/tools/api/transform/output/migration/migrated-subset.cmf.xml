    <RestrictionOf>
      <Datatype structures:ref="xs.token" xsi:nil="true" />
    </RestrictionOf>
  </Datatype>
  <Datatype structures:id="mmucc.VehicleDamageCodeType">
    <Name>VehicleDamageCodeType</Name>
    <Namespace structures:ref="mmucc" xsi:nil="true" />
    <DefinitionText>A data type for events in sequence related to this motor vehicle, including both non-collision as well as collision events.</DefinitionText>
  </Datatype>
