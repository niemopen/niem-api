  <Property structures:id="jc3iedm.EventCategoryCode">
    <Name>EventCategoryCode</Name>
    <Namespace structures:ref="jc3iedm" xsi:nil="true" />
    <DefinitionText>A designator for a category that some event falls under.</DefinitionText>
    <Datatype structures:ref="jc3iedm.EventCategoryCodeType" xsi:nil="true" />
  </Property>
  <Property structures:id="mo.EventCategoryAbstract">
    <Name>EventCategoryAbstract</Name>
    <Namespace structures:ref="mo" xsi:nil="true" />
    <DefinitionText>A data concept for the type or kind of an event.</DefinitionText>
    <AbstractIndicator>true</AbstractIndicator>
  </Property>
  <Property structures:id="mo.SensorIdentification">
    <Name>SensorIdentification</Name>
    <Namespace structures:ref="mo" xsi:nil="true" />
    <DefinitionText>An identification of a sensor.</DefinitionText>
    <Class structures:ref="nc.IdentificationType" xsi:nil="true" />
  </Property>
  <Property structures:id="nc.AircraftCallSignIdentification">
    <Name>AircraftCallSignIdentification</Name>
    <Namespace structures:ref="nc" xsi:nil="true" />
    <DefinitionText>An identification of any combination of characters or pronounceable words, which identifies a communication facility, a command, an authority, an activity, or a unit; used primarily for establishing and maintaining communications.</DefinitionText>
    <Class structures:ref="nc.IdentificationType" xsi:nil="true" />
  </Property>
  <Property structures:id="nc.Person">
    <Name>Person</Name>
    <Namespace structures:ref="nc" xsi:nil="true" />
    <DefinitionText>A human being.</DefinitionText>
    <Class structures:ref="nc.PersonType" xsi:nil="true" />
  </Property>
  <Property structures:id="nc.PersonFullName">
    <Name>PersonFullName</Name>
    <Namespace structures:ref="nc" xsi:nil="true" />
    <DefinitionText>A complete name of a person.</DefinitionText>
    <Datatype structures:ref="nc.PersonNameTextType" xsi:nil="true" />
  </Property>
  <Property structures:id="nc.PersonGivenName">
    <Name>PersonGivenName</Name>
    <Namespace structures:ref="nc" xsi:nil="true" />
    <DefinitionText>A first name of a person.</DefinitionText>
    <Datatype structures:ref="nc.PersonNameTextType" xsi:nil="true" />
  </Property>
  <Property structures:id="nc.PersonMiddleName">
    <Name>PersonMiddleName</Name>
    <Namespace structures:ref="nc" xsi:nil="true" />
    <DefinitionText>A middle name of a person.</DefinitionText>
    <Datatype structures:ref="nc.PersonNameTextType" xsi:nil="true" />
  </Property>
  <Property structures:id="nc.PersonName">
    <Name>PersonName</Name>
    <Namespace structures:ref="nc" xsi:nil="true" />
    <DefinitionText>A combination of names and/or titles by which a person is known.</DefinitionText>
    <Class structures:ref="nc.PersonNameType" xsi:nil="true" />
  </Property>
  <Property structures:id="nc.PersonSurName">
    <Name>PersonSurName</Name>
    <Namespace structures:ref="nc" xsi:nil="true" />
    <DefinitionText>A last name or family name of a person.</DefinitionText>
    <Datatype structures:ref="nc.PersonNameTextType" xsi:nil="true" />
  </Property>
  <Class structures:id="nc.IdentificationType">
    <Name>IdentificationType</Name>
    <Namespace structures:ref="nc" xsi:nil="true" />
    <DefinitionText>A data type for a representation of an identity.</DefinitionText>
  </Class>
  <Class structures:id="nc.PersonNameType">
    <Name>PersonNameType</Name>
    <Namespace structures:ref="nc" xsi:nil="true" />
    <DefinitionText>A data type for a combination of names and/or titles by which a person is known.</DefinitionText>
    <HasProperty>
      <Property structures:ref="nc.PersonGivenName" xsi:nil="true" />
      <MinOccursQuantity>0</MinOccursQuantity>
      <MaxOccursQuantity>unbounded</MaxOccursQuantity>
    </HasProperty>
    <HasProperty>
      <Property structures:ref="nc.PersonMiddleName" xsi:nil="true" />
      <MinOccursQuantity>0</MinOccursQuantity>
      <MaxOccursQuantity>unbounded</MaxOccursQuantity>
    </HasProperty>
    <HasProperty>
      <Property structures:ref="nc.PersonSurName" xsi:nil="true" />
      <MinOccursQuantity>0</MinOccursQuantity>
      <MaxOccursQuantity>unbounded</MaxOccursQuantity>
    </HasProperty>
    <HasProperty>
      <Property structures:ref="nc.PersonFullName" xsi:nil="true" />
      <MinOccursQuantity>0</MinOccursQuantity>
      <MaxOccursQuantity>unbounded</MaxOccursQuantity>
    </HasProperty>
  </Class>
  <Class structures:id="nc.PersonType">
    <Name>PersonType</Name>
    <Namespace structures:ref="nc" xsi:nil="true" />
    <DefinitionText>A data type for a human being.</DefinitionText>
    <HasProperty>
      <Property structures:ref="nc.PersonName" xsi:nil="true" />
      <MinOccursQuantity>0</MinOccursQuantity>
      <MaxOccursQuantity>unbounded</MaxOccursQuantity>
    </HasProperty>
  </Class>
